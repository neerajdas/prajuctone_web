.background
{
background: #c5deea; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-radial-gradient(center, ellipse cover,  hsla(199,47%,85%,1) 0%, hsla(202,49%,69%,1) 31%, hsla(203,93%,35%,1) 100%); /* FF3.6+ */
background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%,hsla(199,47%,85%,1)), color-stop(31%,hsla(202,49%,69%,1)), color-stop(100%,hsla(203,93%,35%,1))); /* Chrome,Safari4+ */
background: -webkit-radial-gradient(center, ellipse cover,  hsla(199,47%,85%,1) 0%,hsla(202,49%,69%,1) 31%,hsla(203,93%,35%,1) 100%); /* Chrome10+,Safari5.1+ */
background: -o-radial-gradient(center, ellipse cover,  hsla(199,47%,85%,1) 0%,hsla(202,49%,69%,1) 31%,hsla(203,93%,35%,1) 100%); /* Opera 12+ */
background: -ms-radial-gradient(center, ellipse cover,  hsla(199,47%,85%,1) 0%,hsla(202,49%,69%,1) 31%,hsla(203,93%,35%,1) 100%); /* IE10+ */
background: radial-gradient(ellipse at center,  hsla(199,47%,85%,1) 0%,hsla(202,49%,69%,1) 31%,hsla(203,93%,35%,1) 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c5deea', endColorstr='#066dab',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */

}

